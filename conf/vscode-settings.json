{
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.rulers": [
    80,
    100
  ],
  "files.associations": {
    "*.am": "makefile",
    "*.tfstate": "json",
    "*.yaml.erb": "yaml",
    "*.yml.erb": "yaml",
    "Puppetfile": "ruby",
    "Vagrantfile": "ruby",
    "Yumfile": "ini"
  },
  "files.exclude": {
    "**/*.pyc": true
  },
  "go.gopath": "/Users/rarmstrong/Development/gocode",
  "go.goroot": "/usr/local/Cellar/go/1.19.5/libexec/",
  "go.useLanguageServer": true,
  "[rust]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
  },
  "[cpp]": {
    "editor.insertSpaces": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.tabSize": 4
  },
  "[c]": {
    "editor.insertSpaces": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.rulers": [ 100 ]
  },
  "[makefile]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false
  },
  "[shellscript]": {
    "editor.tabSize": 4
  },
  "[puppet]": {
    "editor.rulers": [ 140 ]
  },
  "[go]": {
    "editor.tabSize": 4,
    "editor.rulers": [ 100 ],
  },
  "go.toolsManagement.autoUpdate": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.defaultInterpreterPath": "/usr/bin/python3",
  "editor.guides.indentation": true,
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "editor.formatOnSave": true,
  "C_Cpp.default.cppStandard": "c++17",
  "explorer.excludeGitIgnore": true,
  "terminal.integrated.env.osx": {
    "TMUX": "0"
},
}
